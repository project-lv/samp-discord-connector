{
    "user": "project-lv",
    "repo": "samp-discord-connector",
    "resources": [
        {
            "name": "^discord-connector-(.*)-linux.tar.gz$",
            "platform": "linux",
            "archive": true,
            "includes": ["pawno/include"],
            "plugins": ["plugins/discord-connector.so"],
            "files": {
                "log-core2.so": "log-core2.so"
            }
        },
        {
            "name": "^discord-connector-(.*)-win32.zip$",
            "platform": "windows",
            "archive": true,
            "includes": ["pawno/include"],
            "plugins": ["plugins/discord-connector.dll"],
            "files": {
                "log-core2.dll": "log-core2.dll"
            }
        }
    ],
    "runtime": {
        "plugins": ["project-lv/samp-discord-connector"]
    },
    "contributors": [
        "maddinat0r",
        "AGraber",
        "ghost",
        "seanny",
        "Southclaws"
    ]
}
